{
    "name": "solunes/business",
    "description": "Paquete adicional de Negocios para Solunes Digital.",
    "keywords": ["laravel", "solunes", "business"],
    "license": "MIT",
    "authors": [
        {
            "name": "Solunes Digital",
            "email": "info@solunes.com"
        }
    ],
    "require": {
        "ryanwinchester/hubspot-php": "1.*",
        "solunes/master": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/app/Providers",
            "src/app/Controllers",
            "src/app/Console"
       ],
        "psr-4": {
            "Solunes\\Business\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
}
